<?xml version="1.0" encoding="UTF-8"?>
<!--collection of objects with some config -->
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

 <bean id="cafacinoo" class="com.coffee.Cafacinoo">
 
 <property name="cream" value="Y"></property>
 
 </bean>  
 <bean id="xpreso" class="com.coffee.Xpreso">
 
 <constructor-arg name="cream" value="Y"></constructor-arg>
 </bean>


</beans>